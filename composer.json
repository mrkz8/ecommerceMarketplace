{
    "name": "Sistema de Ecommerce marketplace",
    "description": "Store Ecommerce",
    "license": "GNU GENERAL PUBLIC LICENSE Version 2",
    "keywords": [
        "framework",
        "zf2",
        "mkplace"
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "thomasbachem/php-ga",
                "version": "1.2",
                "dist": {
                    "url": "https://github.com/thomasbachem/php-ga/archive/v1.2.zip",
                    "type": "zip"
                },
                "autoload": {
                    "psr-4": {
                        "UnitedPrototype\\"         : "src"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "ocramius/proxy-manager"        : "0.*",
        "zendframework/zendframework"   : "2.*",
        "doctrine/common"               : "2.*",
        "doctrine/doctrine-orm-module"  : "*",
        "doctrine/data-fixtures"        : "*",
        "zendframework/zendservice-recaptcha" : "*",
        "twig/twig"                     : "1.*",
        "thomasbachem/php-ga"           : "1.2",
        "zf-commons/zfc-twig"           : "dev-master",
        "symfony/event-dispatcher"      : "*",
        "symfony/http-kernel"           : "*",
        "symfony/process"               : "*",
        "psr/log"                       : "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "bjyoungblood/bjy-profiler": "dev-master",
        "zendframework/zend-developer-tools" : "dev-master",
        "phploc/phploc": "*",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "2.0.*@dev",
        "pdepend/pdepend" : "2.*"
    },
    "autoload" : {
        "psr-0" : {
            "SanAuth"       : "module/SanAuth/src",
            "Application"   : "module/Application/src"
        }
    }
}
